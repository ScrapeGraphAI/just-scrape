{
	"name": "just-scrape",
	"version": "0.1.1",
	"description": "ScrapeGraph AI CLI tool",
	"type": "module",
	"main": "dist/cli.mjs",
	"bin": {
		"just-scrape": "dist/cli.mjs"
	},
	"scripts": {
		"dev": "bun run src/cli.ts",
		"build": "tsup",
		"lint": "biome check .",
		"format": "biome format . --write",
		"test": "bun test",
		"check": "tsc --noEmit && biome check .",
		"prepublishOnly": "tsup"
	},
	"keywords": ["cli", "scraping", "ai", "scrapegraph"],
	"author": "ScrapeGraph Team",
	"license": "ISC",
	"engines": {
		"node": ">=22"
	},
	"files": ["dist"],
	"dependencies": {
		"@clack/prompts": "^0.10.0",
		"chalk": "^5.4.1",
		"citty": "^0.1.6",
		"dotenv": "^17.2.4",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bun": "^1.3.9",
		"@types/node": "^22.13.1",
		"tsup": "^8.3.6",
		"typescript": "^5.8.2"
	}
}
